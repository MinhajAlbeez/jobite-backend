{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/info/(.*)",
      "dest": "/info/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/**": {
      "maxDuration": 300
    },
    "info/**": {
      "maxDuration": 300
    },
    "api/users/**/*.js": {
      "maxDuration": 30
    }
  }
}
